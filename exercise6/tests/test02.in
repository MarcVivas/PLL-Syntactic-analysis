// Alphabet definition
ALPHABET => a, b <=

// States definition
STATES =>  ->q0, q1->, q2, q4, q5, q6->, q3 <=   // ->q4 is an initial state && q2-> q3-> are final states

// Transitions definitions
TRANSITIONS =>
->q0[a]...q1->;
->q0[b]...q4;
q1->[a]...q4;
q1->[b]...q4;
q2[a]...q3;
q2[b]...q3;
q3[a]...q3;
q3[b]...q2;
q4[a]...q5;
q4[b]...q5;
q5[a]...q2;
q5[b]...q6->;
q6->[a]...q6->;
q6->[b]...q5;
<=
